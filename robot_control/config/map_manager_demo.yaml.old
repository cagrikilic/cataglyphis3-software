grid_map_topic: /control/mapmanager/globalmapviz
grid_map_visualizations:
  - name: sampleProbGrid
    type: occupancy_grid
    params:
     layer: sampleProb
     data_min: 0.0
     data_max: 1000.0
  - name: sampleProbElevation
    type: point_cloud
    params:
     layer: sampleProb
  - name: hazard
    type: occupancy_grid
    params:
     layer: hazard
     data_min: 0.0
     data_max: 1000.0
